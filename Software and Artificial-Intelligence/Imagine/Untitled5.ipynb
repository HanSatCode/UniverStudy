   "source": [
    "n = int(input('숫자 입력: '))\n",
    "resultA = 0 \n",
    "resultB = 0\n",
    "for x in range(1, n+1) :\n",
    "    if x % 2 == 0 :\n",
    "        resultA += x\n",
    "    else :\n",
    "        continue\n",
    "for x in range(1, n+1) :\n",
    "    resultB += 1/x\n",
    "print(resultA, resultB, sep='\\n')"
   ]
