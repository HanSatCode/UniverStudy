   "source": [
    "n = input('문장을 입력하세요:')\n",
    "dictionary = {}\n",
    "for word in n :\n",
    "    dictionary[word] = dictionary.get(word, 0) + 1\n",
    "print(f'Dictionary = {dictionary}')\n",
    "print(f'가장 많이 입력된 문자 = {max(dictionary.keys())} ( 발생 빈도수 = {max(dictionary.values())} )')"
   ]
  }
